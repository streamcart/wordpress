{
    "name": "streamcart/streamcart-for-wordpress",
    "description": "Streamcart Plugin For Wordpress",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "dev-develop",
        "yoast/wp-test-utils": "1.x-dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Streamcart\\WP\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev"
}
